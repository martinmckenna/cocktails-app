{
  "name": "drinks",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.9.1",
    "@material-ui/icons": "^3.0.2",
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "querystring": "^0.2.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.3",
    "react-select": "^2.3.0",
    "recompose": "^0.30.0",
    "throttle-debounce": "^2.1.0",
    "typescript": "3.2.4"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@types/classnames": "^2.2.7",
    "@types/jest": "23.3.13",
    "@types/node": "10.12.18",
    "@types/react": "16.7.20",
    "@types/react-dom": "16.0.11",
    "@types/react-router-dom": "^4.3.1",
    "@types/react-select": "^2.0.11",
    "@types/recompose": "^0.30.3",
    "@types/throttle-debounce": "^1.1.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "tslint-react": "^3.6.0"
  },
  "scripts": {
    "ci:test": "NODE_PATH=./ CI=true react-scripts test",
    "ci:test:coverage": "NODE_PATH=./ CI=true react-scripts test --coverage",
    "start": "BROWSER=none react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "type": "NODE_PATH=./ tsc --noEmit",
    "lint": "NODE_PATH=./ tslint src/**/*.{ts,tsx}",
    "format": "NODE_PATH=./ 'prettier src/**/*.{ts,tsx}' --write"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn format",
      "yarn lint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn ci:test && yarn type && lint-staged && git add ."
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
